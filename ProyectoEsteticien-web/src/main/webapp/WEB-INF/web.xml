<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>ListarTratamientos</servlet-name>
        <servlet-class>com.fpmislata.servlets.ListarTratamientos</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AltaTratamiento</servlet-name>
        <servlet-class>com.fpmislata.servlets.AltaTratamiento</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>EliminarTratamiento</servlet-name>
        <servlet-class>com.fpmislata.servlets.EliminarTratamiento</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ModificarTratamiento</servlet-name>
        <servlet-class>com.fpmislata.servlets.ModificarTratamiento</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ListarTratamientos</servlet-name>
        <url-pattern>/ListarTratamientos</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AltaTratamiento</servlet-name>
        <url-pattern>/AltaTratamiento</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>EliminarTratamiento</servlet-name>
        <url-pattern>/EliminarTratamiento</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ModificarTratamiento</servlet-name>
        <url-pattern>/ModificarTratamiento</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
